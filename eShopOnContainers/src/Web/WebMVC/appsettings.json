{
  "CatalogUrl": "https://catalogapi.pcfbeta.io",
  "OrderingUrl": "https://orderingapi.pcfbeta.io",
  "BasketUrl": "https://basketapi.pcfbeta.io",
  "MarketingUrl": "https://marketingapi.pcfbeta.io",
  "IdentityUrl": "https://identityapi.example.com",
  "CallBackUrl": "https://eshopcontainersmvc.pcfbeta.io",
  "LocationsUrl": "https://locationapi.pcfbeta.io",
  "IsClusterEnv": "True",
  "UseResilientHttp": "False",
  "UseLoadTest": false,
  "ActivateCampaignDetailFunction": "False",
  "UseCustomizationData": false,
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "HttpClientRetryCount": 8,
  "HttpClientExceptionsAllowedBeforeBreaking": 7,
  "spring": {
    "application": {
      "name": "eshopcontainersmvc"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  }
}
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh" ]
        }
      },
      "Cloudfoundry": {
        "ValidateCertificates": false
      }
    }
  },
  "spring": {
    "cloud": {
      "stream": {
        "binder": "rabbit",
        "bindings": {
          "input": {
            "destination": "myDestination",
            "group": "consumerGroup",
            "consumer": {
              "max-attempts": 1,
              "auto-bind-dlq": true,
              "dlq-ttl": 5000,
              "dlq-dead-letter-exchange": ""
            }
          }
        }
      }
    }
  }
}

@model IEnumerable<HrefProperties>

@{
    ViewData["Title"] = "Steeltoe Actuators";
}

<div class="text-center">
    <h1>Available Actuators</h1>
</div>

@if (Model.Count() > 0)
{
    <table class="table text-white text-center">
        <thead>
            <tr>
                <th>Actuator</th>
                <th>Link</th>
            </tr>
        </thead>
        <tbody>
            @foreach (HrefProperties link in Model)
            {
                <tr>
                    <td>@link.Display</td>
                    <td><a class="text-white" href="@link.Address" target="_blank">@link.Address</a></td>
                </tr>
            }
        </tbody>
    </table>
}
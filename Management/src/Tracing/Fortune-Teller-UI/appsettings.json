{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "Steeltoe.Discovery": "None",
      "Fortune_Teller_UI": "Information"
    },
    "Console": {
      "DisableColors": true
    }
  },
  "Spring": {
    "Application": {
      "Name": "fortuneUI"
    }
  },
  "Eureka": {
    "Client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "ShouldRegisterWithEureka": false
    }
  },
  "Management": {
    "Tracing": {
      "AlwaysSample": false
    }
  }
}

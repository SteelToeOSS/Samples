{
  "spring": {
    "cloud": {
      "stream": {
        "binder": "rabbit",
        "bindings": {
          "input": {
            "destination": "so8400in",
            "group": "so8400",
            "consumer": {
              "maxattempts": 1
            }
          }
        },

        "rabbit": {
          "bindings": {
            "input": {
              "consumer": {
                "autoBindDlq": true,
                "republishToDlq": true,
                "dlqTtl": 5000,
                "dlqdeadletterexchange": ""
              }
            }
          }
        }
      }
    }
  }
}
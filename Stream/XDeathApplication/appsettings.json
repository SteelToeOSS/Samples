{
  "spring": {
    "cloud": {
      "stream": {
        "binder": "rabbit",
        "bindings": {
          "input": {
            "destination": "myDestination2",
            "group": "consumerGroup"
          }
        },

        "rabbit": {
          "default": {
            "consumer": {
              "autoBindDlq": true
            }
          },
          "bindings": {
            "input": {
              "consumer": {
                "maxattempts": 1,
                "autoBindDlq": true,
                "dlqTtl": 5000,
                "dlqdeadletterexchange": ""
              }
            }
          }
        }
      }
    }
  }
}
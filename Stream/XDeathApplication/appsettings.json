{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh" ]
        }
      },
      "Cloudfoundry": {
        "ValidateCertificates": false
      }
    }
  },
  "spring": {
    "cloud": {
      "stream": {
        "binder": "rabbit",
        "bindings": {
          "input": {
            "destination": "test4Destination",
            "group": "test4consumerGroup",
            "consumer": {
              "maxAttempts": 1
            }
          }
        },

        "rabbit": {
          "default": {
              "consumer": {
                "autoBindDlq": "true",
                "dlqTtl": 5000,
                "dlqDeadLetterExchange": ""
              }
            }
        }
      }
    }
  }
}
